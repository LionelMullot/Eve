IF(OPTION_BUILD_TYPE_DEBUG AND USING_WINDOWS_MSVC)
# Use VLD
IF(OPTION_BUILD_USE_VLD)
SET(MULTIWINDOW_VLD_API_H FALSE INTERNAL)
FIND_FILE(MULTIWINDOW_VLD_API_H vld.h ${VLD_INCLUDE_DIR})
IF(MULTIWINDOW_VLD_API_H)
SET(HAVE_MULTIWINDOW_VLD_API_H 1 CACHE INTERNAL "Have include ${VLD_INCLUDE_DIR}/vld.h")
ENDIF(MULTIWINDOW_VLD_API_H)
ELSE(OPTION_BUILD_USE_VLD)
SET(HAVE_MULTIWINDOW_VLD_API_H 0 CACHE INTERNAL "Have no include ${VLD_INCLUDE_DIR}/vld.h")
ENDIF(OPTION_BUILD_USE_VLD)
# Use STACKWALKER
IF(OPTION_BUILD_USE_STACKWALKER)
SET(NATIVE_STACKWALKER_API_H FALSE INTERNAL)
FIND_FILE(NATIVE_STACKWALKER_API_H StackWalker.h ${MULTIWINDOW_SOURCE_DIR}/src/executable/)
IF(NATIVE_STACKWALKER_API_H )
SET(HAVE_NATIVE_STACKWALKER_API_H 1 CACHE INTERNAL "Have include ${MULTIWINDOW_SOURCE_DIR}/src/executable/StackWalker.h")
SET(UNHANDLED_EXCEPTION_TEST 1 CACHE INTERNAL "use UNHANDLED_EXCEPTION_TEST")
ENDIF(NATIVE_STACKWALKER_API_H )
ELSE(OPTION_BUILD_USE_STACKWALKER)
SET(HAVE_NATIVE_STACKWALKER_API_H 0 CACHE INTERNAL "Have no include ${MULTIWINDOW_SOURCE_DIR}/src/executable/StackWalker.h")
SET(UNHANDLED_EXCEPTION_TEST 0 CACHE INTERNAL "not use UNHANDLED_EXCEPTION_TEST")     
ENDIF(OPTION_BUILD_USE_STACKWALKER)
ELSE(OPTION_BUILD_TYPE_DEBUG AND USING_WINDOWS_MSVC)
SET(UNHANDLED_EXCEPTION_TEST 0 CACHE INTERNAL "not use UNHANDLED_EXCEPTION_TEST")  
SET(HAVE_MULTIWINDOW_VLD_API_H 0 CACHE INTERNAL "Have no include ${VLD_INCLUDE_DIR}/vld.h")
SET(HAVE_NATIVE_STACKWALKER_API_H 0 CACHE INTERNAL "Have no include ${MULTIWINDOW_SOURCE_DIR}/src/executable/StackWalker.h")
ENDIF(OPTION_BUILD_TYPE_DEBUG AND USING_WINDOWS_MSVC)
